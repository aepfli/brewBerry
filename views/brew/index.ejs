<section id="brewcontrol">
    <h2>BrewControl</h2>
    <ul>
        <% for(var i = 0; i < enums.length; i++) { %>
        <li>
            <span style="background: <%= enums[i].color %>">&nbsp;&nbsp;&nbsp;&nbsp;</span>
            <a href="../events/add?type=<%= enums[i].id %>"><%= enums[i].name %></a>
        </li>
        <% } %>
    </ul>
</section>

